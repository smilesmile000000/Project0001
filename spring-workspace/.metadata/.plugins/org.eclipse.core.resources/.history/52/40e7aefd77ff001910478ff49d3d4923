package org.kh.person.model.vo;

public class Person {
	private String addr;
	private int age;
	private String name;
	public Person() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Person(String addr, int age, String name) {
		super();
		this.addr = addr;
		this.age = age;
		this.name = name;
	}
	public String getAddr() {
		return addr;
	}
	public void setAddr(String addr) {
		this.addr = addr;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
}
