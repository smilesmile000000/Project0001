package org.kh.spring.model.vo;

public class LgTV implements TV {
	public void turnOn() {
		System.out.println("lgTV ---- 전원 켜짐");
	}
	public void turnOff() {
		System.out.println("lgTV ---- 전원 꺼짐");
	}
	public void soundUp() {
		System.out.println("lgTV ---- 소리 올림");
	}
	public void soundDown() {
		System.out.println("lgTV ---- 소리 내림");
	}
}
